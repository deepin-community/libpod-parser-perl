#!/bin/sh

set -e

if [ "$1" = remove ]
then
    dpkg-divert --remove --package libpod-parser-perl --rename \
        --divert /usr/bin/podselect.bundled /usr/bin/podselect

    dpkg-divert --remove --package libpod-parser-perl --rename \
        --divert /usr/share/man/man1/podselect.bundled.1.gz /usr/share/man/man1/podselect.1.gz
fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
